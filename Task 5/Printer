package symbollab1;

/**
 *
 * @author 1
 */
public class Printer {
    
    public Printer() {
    
}
    public synchronized void printH() {
        System.out.print("-");
        notify();
        try{
            wait();
            
        }catch(InterruptedException ex) {
            
        }
    }
    public synchronized void printV() {
        System.out.print("|");
        notify();
        try{
            wait();
        }catch(InterruptedException ex){
            
        }
    }
    
}
